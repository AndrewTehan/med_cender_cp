<h1>Edit Visit</h1>

<%= form_with model: [@medical_center, @department, @doctor, @visit, @inspection_report], local: true do |form| %>
  <p>
    <%= form.label :medical_report %><br>
    <%= form.text_field :medical_report, value: @inspection_report.medical_report %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>

<hr>

<%= link_to "Visit", medical_center_department_doctor_visit_path(@medical_center, @department, @doctor, @visit) %>
